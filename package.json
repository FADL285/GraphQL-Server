{
  "name": "graphql-server-workspace",
  "version": "1.0.0",
  "description": "GraphQL Server project with backend, frontend, and Vue app",
  "private": true,
  "workspaces": [
    "backend",
    "frontend",
    "vue"
  ],
  "scripts": {
    "dev:backend": "npm run dev --workspace=@graphql-server/backend",
    "dev:frontend": "npm run dev --workspace=@graphql-server/frontend",
    "dev:vue": "npm run dev --workspace=@graphql-server/vue",
    "build:backend": "npm run build --workspace=@graphql-server/backend",
    "build:vue": "npm run build --workspace=@graphql-server/vue",
    "install:all": "npm install"
  },
  "keywords": [
    "graphql",
    "workspace",
    "monorepo"
  ],
  "author": "",
  "license": "ISC"
}
